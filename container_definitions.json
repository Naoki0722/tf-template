[
    {
      "name": "nginx",
      "image": "380476085523.dkr.ecr.us-east-2.amazonaws.com/nginx:latest",
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "nginx",
          "awslogs-group": "/ecs/terraform"
        }
      },
      "portMappings": [
        {
          "protocol": "tcp",
          "containerPort": 80
        }
      ]
    }
  ]